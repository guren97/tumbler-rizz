    <script>

        jQuery(document).ready(function () {

            jQuery('form.tax-calculator-form').submit(function (event) {

                jQuery('.form-group').removeClass('has-error');
                jQuery('.help-block').remove();
                jQuery('.card').remove();

                var formData = {
                    'value': jQuery('input[name=value]').val(),
                };

                jQuery.ajax({
                    type: 'POST',
                    url: '../calculate.php',
                    data: formData,
                    dataType: 'json',
                    encode: true
                })
                    .done(function (data) {

                        if (!data.success) {

                            if (data.errors.value) {
                                jQuery('#value-group').addClass('has-error');
                                jQuery('#value-group').append('<div class="help-block">' + data.errors.name + '</div>');
                            }
                        } else {

                            jQuery('form.tax-calculator-form').append('<div class="card"><div class="card-body">Tax on Passport: PKR ' +
                                data.passport + '/-' +
                                '  </div></div><div class="card"><div class="card-body">Tax on CNIC: PKR ' +
                                data.cnic + '/-' + '  </div></div>');

                        }
                    });
                event.preventDefault();
            });

        });
    </script>

